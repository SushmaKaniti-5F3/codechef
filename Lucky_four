#include <iostream>
using namespace std;

int main() {
	int t,n,temp,count=0;
	cin>>t;
	while(t--)
	{
	    cin>>n;
	    count=0;
	    temp=n;
	    while(temp>0)
	    {
	        int rem=temp%10;
	        if(rem==4)
	        count++;
	        temp=temp/10;
	    }
	    cout<<count<<"\n";
	}
	return 0;
}
